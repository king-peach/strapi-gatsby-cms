{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "author": {
      "type": "string"
    },
    "orderTime": {
      "type": "datetime"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag",
      "mappedBy": "article"
    },
    "descrition": {
      "type": "text"
    },
    "keywords": {
      "type": "text"
    },
    "slug": {
      "type": "text"
    }
  }
}
